{
  "name": "justincurieux",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "meta:build": "tsc -p ./.ci/tsconfig.json",
    "dev": "bash -c ./.ci/.shell/meta.build.sh && astro dev",
    "start": "npm run \"meta:build\" && astro dev",
    "build": "npm run \"meta:build\" && astro build",
    "preview": "npm run \"meta:build\" && astro preview",
    "astro": "npm run \"meta:build\" && astro",
    "format": "npm run \"meta:build\" && prettier --write .",
    "clean": "npm run \"meta:build\" && rimraf dist .astro node_modules"
  },
  "devDependencies": {
    "@astrojs/image": "^0.17.2",
    "@astrojs/tailwind": "^4.0.0",
    "@types/micromodal": "^0.3.3",
    "@types/uuid": "^9.0.7",
    "astro": "^2.8.0",
    "astro-icon": "^0.8.1",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "rimraf": "^5.0.1",
    "svgo": "2.8.0",
    "tailwindcss-fluid-type": "^2.0.3"
  },
  "dependencies": {
    "@astrojs/preact": "^2.2.2",
    "@fontsource-variable/inter": "^5.0.4",
    "backoff-rxjs": "^7.0.0",
    "heroicons-astro": "^1.0.4",
    "lucide-preact": "^0.292.0",
    "micromodal": "^0.4.10",
    "preact": "^10.17.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.1",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animated": "^1.0.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.1"
  }
}
